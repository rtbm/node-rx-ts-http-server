{
  "extends": [
    "tslint:latest"
  ],
  "rules": {
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "quotemark": false,
    "arrow-parens": false,
    "max-classes-per-file": false,
    "semicolon": [true, "always", "ignore-bound-class-methods"],
    "prefer-const": true,
    "eofline": true,
    "no-var-requires": false
  }
}
